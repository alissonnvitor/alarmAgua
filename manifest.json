{
    "manifest_version": 3,
    "name": "Beba Água",
    "description": "Extensão para te lembrar quando beber água",
    "version": "1.0",
    "icons": {
        "128": "img/icon128.png"
    },
    "action": {
        "default_icon": "img/icon128.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage", 
        "alarms",
        "notifications"
    ]
}
